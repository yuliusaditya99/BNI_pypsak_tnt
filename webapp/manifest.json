{
	"_version": "1.21.0",
	"sap.app": {
		"id": "sap.ui.bni.toolpageapp",
		"type": "application",
		"i18n": {
			"bundleUrl": "i18n/i18n.properties"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_bluecrystal",
			"sap_fiori_3",
			"sap_horizon"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "sap.ui.bni.toolpageapp.view.app",
			"type": "XML",
			"id": "app",
			"async": true
		},
		"dependencies": {
			"minUI5Version": "1.77.0",
			"libs": {
				"sap.m": {},
				"sap.suite.ui.microchart": {
					"lazy": true
				},
				"sap.tnt": {},
				"sap.ui.core": {},
				"sap.ui.layout": {},
				"sap.uxap": {},
				"sap.suite.ui.commons":{}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleUrl": "i18n/i18n.properties",
					"async": true,
					"fallbackLocale": "en",
					"supportedLocales": ["en", "de"],
					"enhanceWith": [
						{
							"bundleUrl": "reuse/appvar1/i18n/i18n.properties",
							"fallbackLocale": "en",
							"supportedLocales": [
								"en", "de"
							]
						},
						{
							"bundleUrl": "reuse/appvar2/i18n/i18n.properties",
							"bundleUrlRelativeTo": "manifest",
							"supportedLocales": [
								"en",
								"de"
							]
						}
					]
				}
			},
			"side": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/sideContent.json"
			},
			"alerts": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/alerts.json"
			},
			"customer": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/customers.json"
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"controlId": "mainContents",
				"viewType": "XML",
				"controlAggregation": "pages",
				"viewPath": "sap.ui.bni.toolpageapp.view",
				"async": true
			},
			"routes": [		
				{
					"pattern": "",
					"name": "project",
					"target": "project"
				},
				{
					"pattern": "ProjectDetail",
					"name": "projectDetail",
					"target": "projectDetail"
				},
				{
					"pattern": "MasterSettings",
					"name": "masterSettings",
					"target": [
						"detailSettings",
						"masterSettings"
					]
				},
				{
					"pattern": "taskDetail/{taskId}",
					"name": "TaskDetail",
					"target": "TaskDetail"
				},
				{
					"pattern": "MasterSettings/ProcessDefinitions",
					"name": "processDefinitions",
					"target": [
						"detailSettings",
						"masterSettings"
					]
				},
				{
					"pattern": "MasterSettings/UserManagement",
					"name": "userManagement",
					"target": [
						"userManagement",
						"masterSettings"
					]
				}
			],
			"targets": {
			
				"project": {
					"viewId": "project",
					"viewName": "Project"
				},
				"TaskDetail": {
					"viewName": "ProjectDetail",
					"viewType": "XML",
					"viewLevel": 2
				},			
				"appSettings": {
					"viewId": "appSettings",
					"viewName": "settings.AppSettings"
				},
				"masterSettings": {
					"viewId": "masterSettings",
					"viewName": "settings.MasterSettings",
					"controlId": "appSettings",
					"controlAggregation": "masterPages",
					"parent": "appSettings"
				},
				"detailSettings": {
					"viewId": "detailSettings",
					"viewName": "settings.DetailSettings",
					"controlId": "appSettings",
					"controlAggregation": "detailPages",
					"parent": "appSettings"
				},
				"userManagement": {
					"viewId": "userManagement",
					"viewName": "settings.UserManagement",
					"controlId": "appSettings",
					"controlAggregation": "detailPages",
					"parent": "appSettings"
				}
			}
		},
		"resources": {
			"css": [
						{
							"uri": "css/style.css"
						}	
			]
		}
	}
}